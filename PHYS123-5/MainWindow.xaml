<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PHYS123_5"
        xmlns:Chart="clr-namespace:Chart;assembly=Chart" x:Class="PHYS123_5.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="Auto" Width="Auto">
    <StackPanel Orientation="Vertical">
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="87" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="488" FontFamily="Courier New" />

        <Chart:Chart Name="Chart" Width="600" Height="400" XMin="0" XMax="220" YMin="-10" YMax="80" NX = "11" NY="18"/>

    </StackPanel>
</Window>
